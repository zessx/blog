---
layout: null
---
{
  "posts" : [{% for post in site.posts %}
    {
      "title":  "{{ post.title }}",
      "date":  "{{ post.date }}",
      "update":  "{{ post.update }}",
      "url":  "{{ post.url }}",
      "tags": [{% for tag in post.tags %}
        "{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}
      ]
    }{% unless forloop.last %},{% endunless %}{% endfor %}
  ]
}